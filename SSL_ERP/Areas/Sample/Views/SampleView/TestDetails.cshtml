
<script src="~/areas/cs/scripts/testscripts/testdetails.js"></script>

<div class="uk-grid" data-uk-grid-margin>
    <div class="uk-width-medium-1-2 uk-row-first">
        <div class="md-card">
            <div class="md-card-content">
                <h2 class="heading_a uk-margin-medium-bottom">Basic</h2>
                <div class="uk-form-row">
                    <div class="uk-grid" data-uk-grid-margin>
                        <div class="uk-width-medium-1-1">
                            <label>Vendor Code</label>
                            <input type="text" required class="md-input" />
                        </div>
                        <div class="uk-width-medium-1-1">
                            <div class="uk-width-medium-1-1">
                                <label>Vendor Name</label>
                                <input type="text" required class="md-input" />
                            </div>
                        </div>
                        <div class="uk-width-medium-1-1">
                            <div class="md-input-wrapper md-input-filled">
                                <label>Vendor Group</label>
                                <select id="select_demo_2" class="md-input" data-uk-tooltip="{pos:'top'}">
                                    <option value="" disabled="" selected="" hidden="">--Select Vendor Group--</option>
                                    <option value="a">Item A</option>
                                    <option value="b">Item B</option>
                                    <option value="c">Item C</option>
                                </select><span class="md-input-bar "></span>
                            </div>
                        </div>
                        <div class="uk-width-medium-1-1">
                            <div class="uk-input-group">
                                <span class="uk-input-group-addon"><i class="uk-input-group-icon uk-icon-calendar"></i></span>
                                <label for="uk_dp_1">Start Date</label>
                                <input class="md-input" type="text" id="uk_dp_1" required data-uk-datepicker="{format:'DD.MM.YYYY'}">
                            </div>
                        </div>
                        <div class="uk-width-medium-1-1">
                            <div class="uk-width-medium-1-1">
                                <label>Vat Registration No</label>
                                <input type="text" class="md-input" />
                            </div>
                        </div>
                        <div class="uk-width-medium-1-1">
                            <div class="uk-width-medium-1-1">
                                <label>TIN No</label>
                                <input type="text" class="md-input" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="uk-width-medium-1-2">
        <div class="md-card">
            <div class="md-card-content">
                <h2 class="heading_a uk-margin-medium-bottom">Details</h2>
                <div class="uk-form-row">
                    <div class="uk-grid" data-uk-grid-margin>
                        <div class="uk-width-medium-1-1">
                            <label>Address</label>
                            <input type="text" class="md-input" />
                        </div>
                        <div class="uk-width-medium-1-1">
                            <div class="uk-width-medium-1-1">
                                <label>City</label>
                                <input type="text" class="md-input" />
                            </div>
                        </div>
                        <div class="uk-width-medium-1-1">
                            <div class="uk-width-medium-1-1">
                                <label>Country</label>
                                <input type="text" class="md-input" />
                            </div>
                        </div>
                        <div class="uk-width-medium-1-1">
                            <div class="uk-width-medium-1-1">
                                <label>Email</label>
                                <input type="text" class="md-input" />
                            </div>
                        </div>
                        <div class="uk-width-medium-1-1">
                            <div class="uk-width-medium-1-1">
                                <label>Fax No</label>
                                <input type="text" class="md-input" />
                            </div>
                        </div>
                        <div class="uk-width-medium-1-1">
                            <div class="uk-width-medium-1-1">
                                <label>Telephone No</label>
                                <input type="text" class="md-input" />
                            </div>
                        </div>

                        <div class="uk-width-medium-1-1">
                            <div class="uk-width-medium-1-1">
                                <label>Comments</label>
                                <textarea cols="30" rows="2" class="md-input autosized" style="overflow-x: hidden; overflow-wrap: break-word; height: 50px;"></textarea>
                            </div>
                        </div>
                        <div class="uk-width-medium-1-1">
                            <div class="uk-width-medium-1-1">
                                <label>Short Name</label>
                                <textarea cols="30" rows="2" class="md-input autosized" style="overflow-x: hidden; overflow-wrap: break-word; height: 50px;"></textarea>
                            </div>
                        </div>
                        <div class="uk-width-medium-1-3">
                            <input type="checkbox" data-switchery checked id="switch_demo_1" />
                            <label for="switch_demo_1" class="inline-label">Is Register</label>
                        </div>
                        <div class="uk-width-medium-1-3">
                            <input type="checkbox" data-switchery checked id="switch_demo_1" />
                            <label for="switch_demo_1" class="inline-label">Is Turnover</label>
                        </div>
                        <div class="uk-width-medium-1-3">
                            <input type="checkbox" data-switchery checked id="switch_demo_1" />
                            <label for="switch_demo_1" class="inline-label">Active Status</label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="uk-grid" data-uk-grid-margin>
    <div class="uk-width-medium-1-2 uk-row-first">
        <div class="md-card">
            <div class="md-card-content">
                <h2 class="heading_a uk-margin-medium-bottom">Business</h2>
                <div class="uk-form-row">
                    <div class="uk-grid" data-uk-grid-margin>
                        <div class="uk-width-medium-1-1">
                            <label>Business Type</label>
                            <input type="text" required class="md-input" />
                        </div>
                        <div class="uk-width-medium-1-1">
                            <div class="uk-width-medium-1-1">
                                <label>Business Code</label>
                                <input type="text" required class="md-input" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="uk-grid" data-uk-grid-margin>
    <div class="uk-width-medium-1-1">
        <div class="md-card">
            <div class="md-card-content">
                <h2 class="heading_a uk-margin-medium-bottom">Contact Person</h2>
                <div class="uk-form-row">
                    <div class="uk-grid" data-uk-grid-margin>
                        <div class="uk-width-medium-1-2">
                            <label>Contact Person</label>
                            <input type="text" required class="md-input" />
                        </div>
                        <div class="uk-width-medium-1-2">
                            <label>CP Designation</label>
                            <input type="text" required class="md-input" />
                        </div>
                    </div>
                </div>
                <div class="uk-form-row">
                    <div class="uk-grid" data-uk-grid-margin>
                        <div class="uk-width-medium-1-2">
                            <label>CP Telephone</label>
                            <input type="text" required class="md-input" />
                        </div>
                        <div class="uk-width-medium-1-2">
                            <label>CP Email</label>
                            <input type="text" required class="md-input" />

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




