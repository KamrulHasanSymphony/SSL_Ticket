    <script>
        $(document).ready(function (e) {
            loadPartialView();

            // Add click event listener to the MIS Report link
            $('.misnav-link').click(function () {
                // Toggle the visibility of the dropdown menu
                $('.dropdown-menu').toggle();
            });

        });

        $('body').on('click', '.has-treeview', function (e) {
            var isExpanded = $(this).hasClass('treeview-expanded');
            $('.has-treeview.treeview-expanded').not(this).removeClass('treeview-expanded').next('ul').slideUp();

            $(this).next('ul').slideToggle();
            $(this).toggleClass('treeview-expanded', !isExpanded);
        });

        $('.MisBankk').click(function () {

            //$('#ledgerModal').modal('show');
            var url = "/Common/MISLedgerModal?transactionType=Bank";
            modalInit(url);
        });
        $('.MisCashh').click(function () {

            //$('#ledgerModal').modal('show');
            var url = "/Common/MISLedgerModal?transactionType=Cash";
            modalInit(url);
        });
        function loadPartialView() {
            $.get('@Url.Action("LeftSideBar", "Home")', function (data) {
                $('#partialViewContainer').html(data);
            });
        };

